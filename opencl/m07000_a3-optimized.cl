  salt_buf1[3] = 0;
  salt_buf2[0] = 0;
  salt_buf2[1] = 0;
  salt_buf2[2] = 0;
  salt_buf2[3] = 0;
  salt_buf3[0] = 0;
  salt_buf3[1] = 0;
  salt_buf3[2] = 0;
  salt_buf3[3] = 0;

  const u32 salt_len = 12; // salt_bufs[SALT_POS_HOST].salt_len;

  u32 magic_buf0[4];
  u32 magic_buf1[4];
  u32 magic_buf2[4];
  u32 magic_buf3[4];

  magic_buf0[0] = hc_swap32_S (FORTIGATE_A);
  magic_buf0[1] = hc_swap32_S (FORTIGATE_B);
  magic_buf0[2] = hc_swap32_S (FORTIGATE_C);
  magic_buf0[3] = hc_swap32_S (FORTIGATE_D);
  magic_buf1[0] = hc_swap32_S (FORTIGATE_E);
  magic_buf1[1] = hc_swap32_S (FORTIGATE_F);
  magic_buf1[2] = 0;
  magic_buf1[3] = 0;
  magic_buf2[0] = 0;
  magic_buf2[1] = 0;
  magic_buf2[2] = 0;
  magic_buf2[3] = 0;
  magic_buf3[0] = 0;
  magic_buf3[1] = 0;
  magic_buf3[2] = 0;
  magic_buf3[3] = 0;

  const u32 magic_len = 24;

  const u32 salt_pw_len = salt_len + pw_len;

  switch_buffer_by_offset_be_S (magic_buf0, magic_buf1, magic_buf2, magic_buf3, salt_pw_len);

  salt_buf0[0] |= magic_buf0[0];
  salt_buf0[1] |= magic_buf0[1];
  salt_buf0[2] |= magic_buf0[2];
  salt_buf0[3] |= magic_buf0[3];
  salt_buf1[0] |= magic_buf1[0];
  salt_buf1[1] |= magic_buf1[1];
  salt_buf1[2] |= magic_buf1[2];
  salt_buf1[3] |= magic_buf1[3];
  salt_buf2[0] |= magic_buf2[0];
  salt_buf2[1] |= magic_buf2[1];
  salt_buf2[2] |= magic_buf2[2];
  salt_buf2[3] |= magic_buf2[3];
  salt_buf3[0] |= magic_buf3[0];
